    function toRTime(m){
        var miao=0,Fen= 0,timer;
        Fen = parseInt(m / 60);
        miao = m % 60;
        timer=setInterval(function() {
            $(".span").html('<span>00</span>:<span>'+toZero(Fen)+'</span>:<span class="miao">'+toZero(miao)+'</span>');
            if(Fen==0&&miao==0){
                clearInterval(timer);
                detailsEvent.getGoodInfo();
            }
            if(Fen>=0){
                miao--;
                if(miao<0){
                    miao=59;
                }
                if(miao>=0&&miao!=60){
                    if(miao==59){
                        Fen--
                    }
                }
            }
        }, 1000);
    }

    function formatSeconds(value) {
        var theTimeS = parseInt(value);// 秒
        var theTimeM = 0;// 分
        var theTimeH = 0;// 小时
        if(theTimeS >= 60) {
            theTimeM = parseInt(theTimeS/60);
            theTimeS = parseInt(theTimeS%60);
            if(theTimeM >= 60) {
                theTimeH = parseInt(theTimeM/60);
                theTimeM = parseInt(theTimeM%60);
            }
        }
        var result = ""+toZero(parseInt(theTimeS));
        if(theTimeM >= 0) {
            result = ""+toZero(parseInt(theTimeM))+":"+result;
        }
        if(theTimeH >= 0) {
            result = ""+toZero(parseInt(theTimeH))+":"+result;
        }
        return result;
    }

    function time(){
        console.log(formatSeconds(detailsEvent.refundTime));
        $(".refund-time").html(formatSeconds(detailsEvent.refundTime));
        if (detailsEvent.refundTime == 0) {
            if((channel=='Ios1'||channel=='Ios2'||channel=='IosJsjx'||channel=='IosXyjp')&&verCode==111){
                $(".refund-time-p").html("本次推荐热卖专区已结束，敬请期待下次开启");
            }else{
                $(".refund-time-p").html("本次不中必返已结束，敬请期待下次开启");
            }
            
            $(".refund-start-btn").hide();
            $(".refund-time").hide()
        }else{
            detailsEvent.refundTime--;
            //定时器，每秒重新计算
            setTimeout(function() {
                time();
            },1000)
        }
    }

    function newTime(){
        $(".new-hand-time").html(formatSeconds(detailsEvent.newHandTime));
        if (detailsEvent.newHandTime == 0) {
            $(".new-hand-p").text('本商品为新手福利商品，时间已经结束');
            $(".new-hand-time-p").hide();
            $(".new-hand-time").hide()
        }else{
            detailsEvent.newHandTime--;
            //定时器，每秒重新计算
            setTimeout(function() {
                newTime();
            },1000)
        }
    }

    function toZero(num) {
        if (num <= 9) {
            return "0" + num;
        } else {
            return "" + num;
        }
    }


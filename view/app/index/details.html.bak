    var topIndex=0;
    function auto(index,barrage_list){
        var boxDom=$(".barrage-play-item"),pageW=parseInt($(document).width()),barrageTop=["1.5rem","2.5rem","4.3rem"],
                barrageColor=["#cb463c","#5c7182","#f4702e","#a54a50","#657c40","#7d5b4f"];
        var imgOrSpanHtml = "",colorIndex = "";
        topIndex ++;
        if(topIndex>2){
            topIndex=0;
        }
        colorIndex = parseInt(barrageColor.length*(Math.random()));
        if(barrage_list[index].head == ""){
            if(barrage_list[index].nickname.replace(/[^\u4e00-\u9fa5]/gi,"")){
                imgOrSpanHtml = '<span>'+barrage_list[index].nickname.replace(/[^\u4e00-\u9fa5]/gi,"").substring(0,1)+'</span>'
            }else if(barrage_list[index].nickname.replace(/[^A-Za-z]/gi,"")){
                imgOrSpanHtml = '<span>'+barrage_list[index].nickname.replace(/[^A-Za-z]/gi,"").substring(0,1)+'</span>'
            }else {
                imgOrSpanHtml = '<img src="../../../static/img/details/default-head.png">'
            }
        }else {
            imgOrSpanHtml = '<img src="'+barrage_list[index].head+'">'
        }
        var creSpan=$('<div class="barrage-item">'+imgOrSpanHtml+barrage_list[index].content+'</div>');

        boxDom.append(creSpan);
        creSpan.css({"top":barrageTop[topIndex],"left":pageW});
        $(".barrage-play-item>.barrage-item:last-child span").css({"background-color": barrageColor[colorIndex]});
        console.log(creSpan.width());
        var spanDom=$(".barrage-play-item>.barrage-item:last-child");
        spanDom.stop().animate({"left":-creSpan.width()},17000,"linear",function(){
            $(this).remove();
        });
    }

